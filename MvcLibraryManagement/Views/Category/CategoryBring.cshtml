@model MvcLibraryManagement.Models.Entity.TblCategory
@{
    ViewBag.Title = "CategoryBring";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("CategoryUpdate", "Category", FormMethod.Post))
{
    <div style="margin-top:15px;"></div>
    <div class="form-group">
        @Html.LabelFor(k => k.ID)
        @Html.TextBoxFor(k => k.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k=>k.Name)
        @Html.TextBoxFor(k=>k.Name, new {@class="form-control" })
    </div>
    <div>
        <button class="btn btn-success">Update</button>
    </div>
}
