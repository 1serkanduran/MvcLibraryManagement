@model MvcLibraryManagement.Models.Entity.TblStaff
@{
    ViewBag.Title = "StaffAdd";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<form class="form-group" method="post">
    <div>
        <label>Staff Name</label>
        @*<input type="text" class="form-control" name="Staff" />*@
        @Html.TextBoxFor(x => x.Staff, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.Staff, "", new { @style = "color:red" })
    </div>
    <div style="margin-top:15px">
        <button class="btn btn-info" id="btn1">Staff Add</button>
    </div>
</form>

